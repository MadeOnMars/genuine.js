<meta name="description" content="<%= data.description %>">
<meta property="og:url" content="<%= data.fullUrl %>">
<meta property="og:site_name" content="<%= data.description %>">
<meta property="og:title" content="<%= data.title %>">
<meta property="og:description" content="<%= data.description %>">
<meta property="og:image" content="<%= (data.shareImg) || (locals.config.host + '/img/logo.png') %>">
<meta property="og:image:type" content="image/png">
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="630">
<meta property="twitter:site" content="Genuine">
<meta property="twitter:creator" content="Genuine">
<meta property="twitter:card" content="summary_large_image">
